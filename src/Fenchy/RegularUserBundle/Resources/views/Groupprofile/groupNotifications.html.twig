		function changeStatus(status,id,notice_id)
		{
			//alert(status +"=>"+id);			
			
			 $.post('{{path('fenchy_regular_user_listing_change_status')}}',
		                {request_id: id, status: status, notice_id: notice_id}, 
		            function(response){
				        if(response.success == 'Piece Completed') 
				        {
				        	$('#complete_piece').click();
				        } 
				        else if(response.success =='Spot Completed')
				        {
				        	$('#complete_spot').click();
				        }
				        else if(response.success == 'Closed')
                                        {
					        $('#listingClosed').click();
                                        }
				        else
				        {   
			               	var str = response.success.split("^^");
							$('#redtitle1'+id).html(str[2]);
							$('#redtitle'+notice_id).html(str[1]);
							$('#status'+id).html(str[0]);
							
							if(str[2] == 'done')
							{
								//alert(str[3]);
								$.session.set("amount",str[3]); 
								$.session.set("currency",str[4]);
								$('#ReviewWriteBtn'+notice_id).show();
                                                                $('#ReviewWriteBtn'+id).show();
								if(str[5] == 'closed')
								{
									$('#ListItAgain'+notice_id).show();
						        	$('#Repost'+notice_id).hide();
								}
								if(typeof str[6] != 'undefined')
								{
									var data = str[6].split("**");
									var accountType = data[0];
									var cardNo = data[1];
									var endMonth = data[2];
									var endYear = data[3];	
									var accountNo = data[4];
									var bankCode = data[5];	
									var accountHolder = data[6];
									var cardHolder = data[7];
									var cvv_code =  data[8];
                                                                        var paypalEmail = data[9];
								}
								else
								{
									{% if usergroup.paymentId %}
										var accountType = '{{ usergroup.paymentId.type }}';
										var cardNo = '{{ usergroup.paymentId.cardNo }}';
										var endMonth = '{{ usergroup.paymentId.endMonth }}';
										var endYear = '{{ usergroup.paymentId.endYear }}';	
										var accountNo = '{{ usergroup.paymentId.accountNo }}';
										var bankCode = '{{ usergroup.paymentId.bankCode }}';	
										var accountHolder = '{{ usergroup.paymentId.accountHolder }}';
										var cardHolder = '{{ usergroup.paymentId.cardHolder }}';
										var cvv_code =  '{{ cvv_code }}';
                                                                                 var paypalEmail = '{{ usergroup.paymentId.paypalEmail }}';
									{% else %}
										var accountType = "";
										var cardNo = "";
										var endMonth ="";
										var endYear = "";
										var accountNo ="";
										var bankCode = "";
										var accountHolder ="";
										var cardHolder = "";
										var cvv_code ="";
                                                                                var paypalEmail = "";
									{% endif %}
								}
								if(accountType=='credit' && str[3]>0)
								{
									try {
										paymill.createToken({
									 	 	number:         cardNo,
									  		exp_month:      endMonth,
									  		exp_year:       endYear,
									  		cvc:            cvv_code,
									  		amount_int:     str[3],
											currency:       str[4],
									  		cardholder:     cardHolder
										},
										paymillResponseHandler);
									} catch(e) {
								    	logResponse(e.message);
									}
								}
								if(accountType=='debit' && str[3]>0)
								{
									try {
										paymill.createToken({
											number:        accountNo,
											bank:          bankCode,
											accountholder: accountHolder
											},
											paymillResponseHandler);
									} catch(e) {
										logResponse(e.message);
									}
								}
                                                                if(accountType=='paypal' && str[3]>0)
                                                                {
                                                                    
                                                                    var url = "{{ path('fenchy_regular_user_paypalpayment') }}";
                                                                        $.ajax({
                                                                                url : url,
                                                                                type : "post",
                                                                                data : {                                                                                       
                                                                                        'amount': str[3],
                                                                                        'currency': str[4],
                                                                                        'requestId': id,
                                                                                        'noticeId':  notice_id,
                                                                                        'paypalEmail': paypalEmail
                                                                                        },
                                                                                beforeSend : function() {				
                                                                                },
                                                                                success : function(response) {
                                                                                    
                                                                                       var res = response.split('^^^');                                                                                    
                                                                                        if(res[0]!='Fail')
                                                                                            window.location.replace(response); 
                                                                                        else
                                                                                        {
                                                                                            $('#redtitle1'+id).html('accepted');
                                                                                            $('#redtitle'+notice_id).html('accepted');
                                                                                            $('#status'+id).html(res[1]);
                                                                                            $('#ReviewWriteBtn'+id).hide();
                                                                                            $('#WrongPaypal').click();
                                                                                            
                                                                                        }
                                                                                },
                                                                                error : function() {
                                                                                        alert('Something went wrong!');
                                                                                },
                                                                                complete : function() {	
                                                                                        $.session.remove("amount"); 
                                                                                        $.session.remove("currency");				
                                                                                }
                                                                        });
                                                                }
							}
				        }
		    }, "json");  
		}

		function paymillResponseHandler(error, result) {
		    if (error) {
		      // Displays the error above the form
		      alert(error.apierror);
		    } else {		      
		      // Output token
		      var token = result.token;
		      //alert('token=>'+token);
		      var amount = $.session.get("amount"); 
			  var currency = $.session.get("currency");
		       
		      var url = "{{ path('fenchy_regular_user_paymillpayment') }}";
				$.ajax({
					url : url,
					type : "post",
					data : {
						'token': token,
						'amount': amount,
						'currency': currency
						},
					beforeSend : function() {				
					},
					success : function(response) {
						//alert(response);
					},
					error : function() {
						alert('Something went wrong!');
					},
					complete : function() {	
						$.session.remove("amount"); 
						$.session.remove("currency");				
					}
				});
		    }
		}

		function logResponse(res) {
		     // create console.log to avoid errors in old IE browsers
		     if (!window.console) console = {log:function(){}};

		     console.log(res);
		     if(PAYMILL_TEST_MODE)
		         $('.debug').text(res);
		}
