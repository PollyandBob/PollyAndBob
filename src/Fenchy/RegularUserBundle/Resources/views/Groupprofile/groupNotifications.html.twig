		function writeReview(noticeId, requestId){
			
			var text = $('#reviewtext'+requestId).val();
                        //alert(text);
		    $.post('{{path('fenchy_notice_write_review')}}',
		                {noticeId: noticeId, text: text, requestId: requestId, flag: false}, 
		            function(response){
		                $('#reviewReplace'+noticeId).html(response.success);
		                $('#reviewtext'+requestId).val("");
				$('.reviewForm'+requestId).hide();
                                $('#ReviewWrite'+requestId).hide();
                                $('#ReviewWriteBtn'+requestId).hide();

		    }, "json");    
		}
